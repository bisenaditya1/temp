import java.util.Arrays;

public class Program {
	
	private static void rightrotation(int[] arr, int n) {
		int temp = arr[arr.length-1]
		for (int i=0; i<n-2; i++) {
			arr[i+1]=arr[i];
		}
		arr[0]=temp;
		System.out.println(Arrays.toString(arr));
	}


	public static void main(String[] args) {
		int [] arr = {1,4,7,3,9,6};
		int n = arr.length;
		rightrotation(arr,n);

	}
}

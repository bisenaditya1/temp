
public class Program {
	
	private static void twoSum(int[] arr, int target, int n) {
		for(int i=0; i<n; i++) {
			for(int j=0; j<n; j++) {
				if(arr[i]+arr[j]==target)
					System.out.println("yes twoSum exists");
			}
		}
	}

	

	public static void main(String[] args) {
		int [] arr = {2,6,5,8,11};
		int target = 4;
		int n = arr.length;
		twoSum(arr, target, n);
	}

}

import java.util.Arrays;

public class Program {

	private static void missingElement(int[] arr, int n, int size) {
		int [] dummy = new int [n];
		for(int i=0; i<n; i++) {
			dummy [i]= i+1;
		}
	}

	public static void main(String[] args) {
		int[] arr = { 1, 3, 4, 5, 6, 7 };
		Arrays.sort(arr);
		int size = arr.length;
		int n = 7;
		missingElement(arr, n, size);
	}

}


public class Program {

	private static void singleOccurance(int[] arr, int n) {
		for (int i = 0; i < n; i++) {
			int count = 1;
			for (int j = 1; j < n; j++) {
				if (arr[i] == arr[j]) {
					count++;
					break;
				}
			}
			if ( count == 1)
				System.out.println("Jii");

		}
	}

	public static void main(String[] args) {
		int[] arr = { 1, 3, 2, 1, 2, 3, 4 };
		int n = arr.length;
		singleOccurance(arr, n);
	}

}

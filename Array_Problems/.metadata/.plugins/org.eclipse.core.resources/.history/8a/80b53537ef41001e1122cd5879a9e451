import java.util.Arrays;

public class Program {
	

	private static int missingElement(int[] arr, int n, int size) {
		for(int i=0; i<size; i++) {
			if ( arr[i]!=i+1)
				return (i+1);
			else
				return 0;
		
	}


	public static void main(String[] args) {
		int [] arr = {1,3,4,5,6,7};
		Arrays.sort(arr);
		int size = arr.length;
		int n=7;
		int missing = missingElement(arr, n, size);
	}

}
